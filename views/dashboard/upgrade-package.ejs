                                <i class="nav-link-icon fa fa-cog"></i>
                                Business Settings
                            </a>
                        </li>
                    </ul> -->
                </div>
                <div class="app-header-right">
                    <div class="header-btn-lg pr-0">
                        <div class="widget-content p-0">
                            <div class="widget-content-wrapper">
                                <div class="widget-content-left">
                                    <div class="btn-group">
                                        <a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"
                                            class="p-0 btn">
                                            <img width="42" class="rounded-circle" src="assets/images/avatars/user.png"
                                                alt="">
                                            <i class="fa fa-angle-down ml-2 opacity-8"></i>
                                        </a>
                                        <div tabindex="-1" role="menu" aria-hidden="true"
                                            class="dropdown-menu dropdown-menu-right">
                                            <!-- <button type="button" tabindex="0" class="dropdown-item">User
                                                Account</button>
                                            <button type="button" tabindex="0" class="dropdown-item">Settings</button> -->
                                            <a href="/logout" type="button" tabindex="0"
                                                class="dropdown-item">Logout</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="widget-content-left  ml-3 header-user-info">
                                    <div class="widget-heading">
                                        <%= fullname %>
                                    </div>
                                    <div class="widget-subheading">
                                        <%= email %>
                                    </div>
                                </div>
                                <!-- <div class="widget-content-right header-user-info ml-3">
                                    <button type="button"
                                        class="btn-shadow p-1 btn btn-primary btn-sm show-toastr-example">
                                        <i class="fa text-white fa-calendar pr-1 pl-1"></i>
                                    </button>
                                </div> -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                                data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button"
                            class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">
                            <li class="app-sidebar__heading">Dashboard</li>
                            <li>
                                <a href="/dashboard">
                                    <i class="metismenu-icon pe-7s-rocket"></i>
                                    Dashboard
                                </a>
                            </li>

                            <li class="app-sidebar__heading">Message</li>
                            <li>

                                <a href="#">
                                    <i class="metismenu-icon pe-7s-eyedropper"></i>
                                    Elements
                                    <i class="metismenu-state-icon pe-7s-angle-down caret-left"></i>
                                </a>
                                <ul>
                                    <!-- <li>
                                        <a href="/import-contacts">
                                            <i class="metismenu-icon"></i>
                                            Import Contacts
                                        </a>
                                    </li> -->
                                    <li>
                                        <a href="/send-message">
                                            <i class="metismenu-icon">
                                            </i>Send Message
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/sent-messages">
                                            <i class="metismenu-icon">
                                            </i>Sent Messages
                                        </a>
                                    </li>
                                </ul>
                            </li>

                            <!-- <li class="app-sidebar__heading">Contacts</li>
                            <li>
                                <a href="/imported-contacts">
                                    <i class="metismenu-icon pe-7s-display2"></i>
                                    Imported Contacts
                                </a>
                            </li> -->

                            <li class="app-sidebar__heading">Buy SMS Credits</li>
                            <li>
                                <a href="/upgrade-package">
                                    <i class="metismenu-icon pe-7s-graph2">
                                    </i>
                                    Upgrade Package
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                <div class="app-main__inner">
                    <div class="app-page-title">
                        <div class="page-title-wrapper">
                            <div class="page-title-heading">
                                <div class="page-title-icon">
                                    <i class="pe-7s-graph2 icon-gradient bg-mean-fruit">
                                    </i>
                                </div>
                                <div>Upgrade Package
                                </div>
                            </div>
                            <div class="page-title-actions">
                                <button type="button" data-toggle="tooltip" title="User Level" data-placement="bottom"
                                    class="btn-shadow mr-3 btn btn-dark">
                                    <i class="fa fa-star"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="row">


                    </div>

                    <div class="row">

                        <div class="col-lg-12 col-xl-12">
                            <div class="card mb-3 widget-content">
                                <div class="widget-content-outer">
                                    <div class="widget-content-wrapper">
                                        <div class="widget-content-left">
                                            <h5 class="card-title">Telephone:</h5>
                                            <div class="widget-subheading">Contact the telephone number to buy more SMS Credits</div>
                                        </div>
                                        <div class="widget-content-right">
                                            <div class="widget-numbers text-danger">0559687893</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="app-wrapper-footer">
                    <div class="app-footer">
                        <div class="app-footer__inner">
                            <div class="app-footer-left">
                                <ul class="nav">
                                    <li class="nav-item">
                                        <a href="https://architect-messenger.herokuapp.com" class="nav-link">
                                            Architect Messenger
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link">
                                            Copyright &copy;
                                            <script> document.write(new Date().getFullYear());</script>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="app-footer-right">
                                <ul class="nav">
                                    <li class="nav-item">
                                        <a href="https://maverick-edifice.herokuapp.com" target="_blank"
                                            class="nav-link">
                                            <div class="badge badge-success mr-1 ml-0">
                                                <small>Powered By:</small>
                                            </div>
                                            Maverick Edifice
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="../assets/scripts/main.js"></script>

    <!-- IZITOAST -->
    <script src="../assets/izitoast/js/iziToast.min.js"></script>

    <% if( error != '') { %>
    <script>
        var notify = new Audio('../assets/sounds/notify.mp3');
        notify.play();
        iziToast.error({ title: 'Error!', message: `<%= error %>`, position: 'bottomLeft' })
    </script>
    <% } %>
    <% if(error_msg != '') { %>
    <script>
        var notify = new Audio('../assets/sounds/notify.mp3');
        notify.play();
        iziToast.info({ title: 'Info!', message: `<%= error_msg %>`, position: 'bottomLeft' })
    </script>
    <% } %>
    <% if(success_msg != '') { %>
    <script>
        var notify = new Audio('../assets/sounds/notify.mp3');
        notify.play();
        iziToast.info({ title: 'Info!', message: `<%= success_msg %>`, position: 'bottomLeft' })
    </script>
    <% } %>
</body>

</html>